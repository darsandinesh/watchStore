            $.ajax({
                url: '/displayaddress',
                data: {
                    addressId: data
                },
                method: 'post',
                dataType: 'json',
                success: (response) => {
                    if (response) {
                        console.log(response.data.fullname)
                        console.log(response)
                        document.getElementById('name').value = response.data.fullname
                        document.getElementById('phone').value = response.data.phone
                        document.getElementById('house').value = response.data.address.houseName
                        document.getElementById('city').value = response.data.address.city
                        document.getElementById('state').value = response.data.address.state
                        document.getElementById('pincode').value = response.data.address.pincode
                        document.getElementById('country').value = response.data.address.country

                    } else {
                        console.log('somthing went wrong!!')
                    }
                }
            })
        }
