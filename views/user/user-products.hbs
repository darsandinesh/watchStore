                                                                                    {{#if this.offer}}
                                                                            <span class="price"><del>{{this.price}}</del>&nbsp;{{this.discountAmount}}Rs.
                                                                            </span>
                                                                            {{else}}
                                                                            <span class="price">Rs.{{this.price}}</span>
                                                                            {{/if}}
                                                                                </div>
                                                                            </div>
                                                                            <div class="discription">
